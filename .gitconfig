[user]
	email = voyaking@gmail.com
	name = Georgii Voyakin
[core]
	autocrlf = input
	safecrlf = warn
	quotepath = false
	editor = vim
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
